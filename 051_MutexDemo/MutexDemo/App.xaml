<Application x:Class="MutexDemo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MutexDemo"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/WffControls;component/Resource/DrawingImage.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WffControls;component/Resource/Geometry.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WffControls;component/Resource/Brush.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WffControls;component/Resource/Theme.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
